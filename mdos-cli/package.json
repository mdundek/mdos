{
    "name": "mdos-cli",
    "version": "1.5.3",
    "description": "mdos cli tool",
    "author": "Michael Dundek",
    "bin": {
        "mdos": "./bin/dev"
    },
    "homepage": "https://github.com/mdos-cli/hello-world",
    "license": "MIT",
    "main": "dist/index.js",
    "repository": "mdos-cli/hello-world",
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "dependencies": {
        "@feathersjs/feathers": "^4.5.15",
        "@feathersjs/socketio-client": "^4.5.15",
        "@oclif/plugin-help": "^5",
        "@oclif/plugin-not-found": "^2.3.1",
        "@oclif/plugin-plugins": "^2.0.1",
        "adm-zip": "^0.5.9",
        "aws-sdk": "^2.1195.0",
        "axios": "^0.27.2",
        "chalk": "^4.1.2",
        "draftlog": "^1.0.13",
        "inquirer": "^8.2.4",
        "nanoid": "^3.3.4",
        "pkg": "^5.8.0",
        "shelljs": "^0.8.5",
        "socket.io-client": "^2.5.0",
        "treeify": "^1.1.0",
        "yaml": "^2.1.1"
    },
    "devDependencies": {
        "@types/chai": "^4",
        "@types/inquirer": "^9.0.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.9.4",
        "chai": "^4",
        "eslint": "^7.32.0",
        "eslint-config-oclif": "^4",
        "eslint-config-oclif-typescript": "^1.0.2",
        "globby": "^11",
        "mocha": "^9",
        "oclif": "^3.2.2",
        "shx": "^0.3.3",
        "ts-node": "^10.2.1",
        "tslib": "^2.3.1",
        "typescript": "^4.4.3"
    },
    "oclif": {
        "bin": "mdos",
        "dirname": "mdos",
        "commands": "./dist/commands",
        "flexibleTaxonomy": true,
        "hooks": {
            "command_incomplete": "./dist/hooks/command_incomplete.js"
        },
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-not-found"
        ],
        "topicSeparator": " ",
        "topics": {
            "application": {
                "description": "Manage your applications"
            },
            "generate": {
                "description": "Scaffold applications and components"
            },
            "auth": {
                "description": "Manage platform users and roles"
            },
            "shared-volume": {
                "description": "Manage your shared volumes"
            },
            "auth:tenant": {
                "description": "Manage platform tenant scope roles"
            },
            "auth:user": {
                "description": "Assig platform tenant roles to users"
            },
            "namespace": {
                "description": "Manage your kubernetes namespaces"
            },
            "oidc": {
                "description": "Manage OIDC authentication providers and protect your applications"
            },
            "oidc:provider": {
                "description": "Manage OIDC authentication providers and protect your applications"
            },
            "cm": {
                "description": "Manage your Cert-Manager assets"
            },
            "cm:cert": {
                "description": "Manage your Cert-Manager TLS certificates"
            },
            "cm:issuer": {
                "description": "Manage your Cert-Manager Issuers"
            },
            "ingress-gateway": {
                "description": "Configure your namespace specific ingress gateway configurations"
            }
        }
    },
    "scripts": {
        "package": "oclif pack tarballs",
        "package-win": "oclif pack win",
        "build": "shx rm -rf dist && tsc -b && cp -r ./src/lib ./dist",
        "lint": "eslint . --ext .ts --config .eslintrc",
        "postpack": "shx rm -f oclif.manifest.json",
        "posttest": "yarn lint",
        "prepack": "yarn build && oclif manifest && oclif readme",
        "test": "mocha --forbid-only \"test/**/*.test.ts\""
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "bugs": "https://github.com/mdos-cli/hello-world/issues",
    "keywords": [
        "oclif"
    ],
    "types": "dist/index.d.ts"
}
